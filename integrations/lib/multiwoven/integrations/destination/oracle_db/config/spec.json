{
  "documentation_url": "https://docs.squared.ai/guides/destinations/retl-destinations/database/oracle",
  "stream_type": "dynamic",
  "connector_query_type": "raw_sql",
  "connection_specification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Oracle",
    "type": "object",
    "required": ["host", "port", "sid", "username", "password"],
    "properties": {
      "host": {
        "description": "The Oracle host.",
        "examples": ["localhost"],
        "type": "string",
        "title": "Host",
        "order": 0
      },
      "port": {
        "description": "The Oracle port number.",
        "examples": ["1521"],
        "type": "string",
        "title": "Port",
        "order": 1
      },
      "sid": {
        "description": "The name of your service in Oracle.",
        "examples": ["ORCLPDB1"],
        "type": "string",
        "title": "SID",
        "order": 2
      },
      "username": {
        "description": "The username used to authenticate and connect.",
        "type": "string",
        "title": "Username",
        "order": 3
      },
      "password": {
        "description": "The password corresponding to the username used for authentication.",
        "type": "string",
        "multiwoven_secret": true,
        "title": "Password",
        "order": 4
      }
    }
  }
}